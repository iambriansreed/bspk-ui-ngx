{
    "name": "bspk-ui-ngx",
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "lint:css": "npx stylelint '**/*.scss'",
        "lint:css:fix": "npx stylelint '**/*.scss' --fix",
        "ng": "ng",
        "lint": "npx tsx .scripts/lint.ts",
        "start": "npm run prebuild && ng serve",
        "build": "npm run prebuild && ng build",
        "build:all": "npm run prebuild && ng build --project=demo --configuration=production && ng build --project=ui --configuration=production",
        "newc": "ng generate component --project=ui",
        "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
        "dev": "npm run start",
        "prebuild": "npx tsx .scripts/pre-build.ts",
        "gh-build": "npm run prebuild && ng build --project demo --configuration production && cp -r ./dist/browser/index.html ./dist/browser/404.html",
        "pr": "npm run lint && npm run build:all && git push --set-upstream origin $(git symbolic-ref --short HEAD)  && open \"https://github.com/Anywhererealestate/bspk-ui-ngx/compare/main...$(git symbolic-ref --short HEAD)\""
    },
    "private": true,
    "peerDependencies": {
        "@bspk/styles": "^1.0.2"
    },
    "dependencies": {
        "@angular/cdk": "^20.2.9",
        "@angular/common": "^20.3.0",
        "@angular/compiler": "^20.3.0",
        "@angular/core": "^20.3.0",
        "@angular/forms": "^20.3.0",
        "@angular/platform-browser": "^20.3.0",
        "@angular/router": "^20.3.0",
        "@bspk/icons": "^1.0.5",
        "ngx-mask": "^20.0.3",
        "rxjs": "~7.8.0",
        "tslib": "^2.3.0",
        "zone.js": "~0.15.0"
    },
    "devDependencies": {
        "@angular/build": "^20.3.8",
        "@angular/cli": "^20.3.3",
        "@angular/compiler-cli": "^20.3.0",
        "@testing-library/angular": "^18.1.0",
        "@testing-library/jest-dom": "^6.0.0",
        "@types/jest": "^30.0.0",
        "@types/jest-axe": "^3.5.9",
        "jest": "^29.7.0",
        "jest-axe": "^10.0.0",
        "jest-environment-jsdom": "^30.2.0",
        "jest-preset-angular": "^14.6.2",
        "jsdom": "^27.1.0",
        "ng-packagr": "^20.3.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^3.6.2",
        "prettier-plugin-jsdoc": "^1.5.0",
        "stylelint": "^16.19.1",
        "stylelint-config-standard": "^38.0.0",
        "stylelint-config-standard-scss": "^14.0.0",
        "ts-jest": "^29.1.0",
        "ts-node": "^10.9.2",
        "typescript": "~5.9.2",
        "watch": "^0.13.0"
    },
    "overrides": {
        "glob": "^11.0.1"
    }
}
