{
    "name": "bspk-ui-ngx",
    "version": "1.1.4",
    "license": "CC-BY-4.0",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Anywhererealestate/bspk-ui-ngx"
    },
    "engines": {
        "node": ">=22"
    },
    "scripts": {
        "prebuild": "npm run meta && npx tsx .scripts/pre-build.ts",
        "build": "ng build --project=demo --configuration=production && ng build --project=ui --configuration=production",
        "checks": "npm run lint:all && npm run test",
        "dev": "npm run meta && npm run routes && run-p dev:serve dev:watch",
        "dev:serve": "ng serve",
        "dev:watch": "npx tsx .scripts/dev.ts",
        "gh-build": "npm run lint && npm run build && npm run test && cp -r ./dist/browser/index.html ./dist/browser/404.html",
        "lint": "eslint && tsc --noEmit ",
        "lint:all": "npm run lint && npm run lint:ui && npm run lint:css",
        "lint:css": "npx stylelint '**/*.scss'",
        "lint:css:fix": "npx stylelint '**/*.scss' --fix",
        "lint:ui": "npx tsx .scripts/lint.ts",
        "newc": "ng generate component --project=ui && npx tsx .scripts/generate-new-component.ts",
        "ng": "ng",
        "pr": "npm run checks && git push --set-upstream origin $(git symbolic-ref --short HEAD) && open \"https://github.com/Anywhererealestate/bspk-ui-ngx/compare/dev...$(git symbolic-ref --short HEAD)\"",
        "routes": "npx tsx .scripts/generate-component-routes.ts --write",
        "meta": "npx tsx .scripts/generate-meta.ts --write",
        "start": "npm run prebuild && ng serve",
        "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
        "publish:test": "ng build ui && npm pack ./dist/ui",
        "semantic-release": "semantic-release",
        "preview": "npm run gh-build && npx http-server ./dist/browser",
        "clean": "rm -rf ./dist ./coverage ./.angular projects/demo/src/meta.ts projects/demo/src/routes/generated.ts"
    },
    "overrides": {
        "glob": "^11.0.1"
    },
    "dependencies": {
        "@angular/common": "^20.3.0",
        "@angular/compiler": "^20.3.0",
        "@angular/core": "^20.3.0",
        "@angular/platform-browser": "^20.3.0",
        "@angular/router": "^20.3.0",
        "rxjs": "~7.8.0",
        "tslib": "^2.3.0",
        "zone.js": "~0.15.0"
    },
    "devDependencies": {
        "@angular/build": "^20.3.8",
        "@angular/cli": "^20.3.3",
        "@angular/compiler-cli": "^20.3.0",
        "@bspk/icons": "^1.1.1",
        "@bspk/ui": "^1.3.23",
        "@cspell/eslint-plugin": "^9.4.0",
        "@semantic-release/git": "^10.0.1",
        "@semantic-release/github": "^12.0.2",
        "@semantic-release/npm": "^13.1.2",
        "@testing-library/angular": "^18.1.0",
        "@testing-library/jest-dom": "^6.0.0",
        "@types/jest": "^30.0.0",
        "@types/jest-axe": "^3.5.9",
        "angular-eslint": "21.0.1",
        "eslint": "^9.39.1",
        "eslint-plugin-import": "^2.32.0",
        "jest": "^29.7.0",
        "jest-axe": "^10.0.0",
        "jest-environment-jsdom": "^30.2.0",
        "jest-preset-angular": "^14.6.2",
        "jsdom": "^27.1.0",
        "ng-packagr": "^20.3.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^3.6.2",
        "prettier-plugin-jsdoc": "^1.5.0",
        "semantic-release": "^25.0.2",
        "stylelint": "^16.19.1",
        "stylelint-config-standard": "^38.0.0",
        "stylelint-config-standard-scss": "^14.0.0",
        "ts-jest": "^29.1.0",
        "ts-node": "^10.9.2",
        "tslib": "^2.3.0",
        "typescript": "~5.9.2",
        "typescript-eslint": "8.46.4",
        "watch": "^0.13.0"
    },
    "peerDependencies": {
        "@bspk/styles": "^1.0.2",
        "@floating-ui/dom": "^1.7.4"
    }
}
